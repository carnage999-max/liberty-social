{
  "expo": {
    "name": "Liberty Social",
    "slug": "liberty-social",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B3D91"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.libertysocial.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B3D91"
      },
      "package": "com.libertysocial.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "liberty-social",
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#0B3D91",
          "sounds": []
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#0B3D91"
    },
    "extra": {
      "eas": {
        "projectId": "af063308-b482-4d48-8149-c1cef1a1dd13"
      }
    }
  }
}
